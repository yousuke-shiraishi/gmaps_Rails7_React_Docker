    "@mui/icons-material": "^7",
    "@mui/material": "^7",
    "@mui/styled-engine": "^6",
    "@mui/system": "^7",
    "@mui/types": "^7",
    "@react-google-maps/api": "^2",
    "@reduxjs/toolkit": "^2",
    "@types/redux-logger": "^3.0.13",
    "axios": "^1",
    "react": "^19",
    "react-dom": "^19",
    "react-hook-form": "^7",
    "react-icons": "^5",
    "react-modal": "^3",
    "react-redux": "^9",
    "react-router-dom": "^7",
