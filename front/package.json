{
  "name": "front_end",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "start:host": "vite --host",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "start:prod": "vite preview --host",
    "stop": "kill-port 5173 || true",
    "restart": "npm run stop && npm run start",
    "test": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "prettier --write .",
    "repl": "ts-node"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
