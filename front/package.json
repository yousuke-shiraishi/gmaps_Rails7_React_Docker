    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "3.2.4",
    "@welldone-software/why-did-you-render": "^10.0.1",
    "eslint": "^9",
    "eslint-plugin-jest-dom": "^5",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^5",
